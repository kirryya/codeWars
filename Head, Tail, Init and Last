const head = arr => {
  const arrHead = [...arr];
  return arrHead.shift();
}

const tail = arr => {  
  const arrTail = [...arr];
  const shifted = arrTail.shift(); 
  return arrTail;
}

const init = arr => {
  const arrInit = [...arr];
  const popped = arrInit.pop();
  return arrInit;
}

const last = arr => {
  const arrHead = [...arr];
  return arrHead.pop();
}
